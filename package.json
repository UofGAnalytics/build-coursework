{
  "name": "build-coursework",
  "private": true,
  "workspaces": [
    "coursework",
    "template",
    "dev"
  ],
  "repository": "https://github.com/UofGAnalytics/build-coursework.git",
  "author": "David McArthur <david.mcarthur.2@glasgow.ac.uk>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently yarn:coursework yarn:template -c green,cyan",
    "dev2": "babel-node --extensions .ts dev/index.ts",
    "coursework": "yarn workspace coursework dev",
    "template": "yarn workspace template dev",
    "test": "jest --verbose",
    "check-types": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/parser": "^4.14.1",
    "concurrently": "^6.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^24.1.3",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "typescript": "^4.1.3"
  },
  "importSort": {
    ".ts, .tsx, .js, .jsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
